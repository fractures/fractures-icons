{
  "name": "@fractures/icons",
  "version": "0.0.4",
  "author": "Chris Puska <theta@hey.com>",
  "scripts": {
    "docs:dev": "next dev",
    "docs:build": "NODE_ENV=production next build",
    "docs:start": "next start",
    "docs:lint": "next lint",
    "prepublishOnly": "yarn run build && yarn run build:meta",
    "build": "svgo -f ./src/icons -o ./dist --config=./svgo.config.js",
    "build:meta": "ts-node ./meta.ts",
    "test": "svgo --help",
    "figma:export-components": "FIGMA_TOKEN=$(grep FIGMA_TOKEN .env | cut -d '=' -f2) figma-export components $(grep FIGMA_FILE .env | cut -d '=' -f2) -O @figma-export/output-components-as-svg --output ./src"
  },
  "files": [
    "./dist"
  ],
  "main": "./dist",
  "dependencies": {},
  "devDependencies": {
    "@figma-export/cli": "^4.4.0",
    "@figma-export/output-components-as-svg": "^4.4.0",
    "@figma-export/output-styles-as-sass": "^4.4.0",
    "@fractures/css": "^2.0.2",
    "@fractures/ui": "^0.1.46",
    "@swc/core": "^1.2.205",
    "@swc/helpers": "^0.4.2",
    "@types/node": "^17.0.42",
    "@types/react": "^18.0.14",
    "eslint": "8.18.0",
    "eslint-config-next": "12.2.0",
    "fathom-client": "^3.5.0",
    "next": "12.2.0",
    "next-global-css": "^1.3.1",
    "prettier": "2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "regenerator-runtime": "^0.13.9",
    "svgo": "^2.8.0",
    "ts-node": "^10.8.1",
    "typescript": "4.7.3"
  }
}
